# Sudo

alias sudo="sudo " # Enable aliases to be sudoed
alias _="sudo"

# Navigation

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias db="cd ~/Dropbox"
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"

alias mkdir="mkdir -pv"

# Util

alias bc="bc -l"
alias f="find . -iname"
alias g=git
alias h=http
alias v=vagrant

# Ruby

alias be="bundle exec"
alias cop="bundle exec rubocop"
